<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mukul.SearchList">
    <children>
        <VBox layoutX="-1.0" layoutY="-4.0" prefHeight="609.0" prefWidth="295.0" style="-fx-background-color: #ffff66;">
            <children>
                <Group />
                <VBox />
                <Button fx:id="Search" mnemonicParsing="false" onAction="#back" prefHeight="610.0" prefWidth="294.0" style="-fx-background-color: #ffc0cb;" text="BACK" />
            </children>
        </VBox>
        <VBox layoutX="294.0" layoutY="-4.0" prefHeight="94.0" prefWidth="733.0" style="-fx-background-color: #996699;">
            <children>
                <Label alignment="CENTER" blendMode="GREEN" nodeOrientation="LEFT_TO_RIGHT" prefHeight="68.0" prefWidth="734.0" text="Search List" textFill="#16f9f5">
                    <font>
                        <Font name="System Bold" size="32.0" />
                    </font>
                </Label>
            </children>
        </VBox>
        <TableView fx:id="tableView" layoutX="334.0" layoutY="104.0" prefHeight="413.0" prefWidth="653.0">
            <columns>
                <TableColumn fx:id="priceCol" prefWidth="67.0" text="Price" />
                <TableColumn fx:id="countryCol" prefWidth="76.0" text="Seller Name" />
                <TableColumn fx:id="districtCol" prefWidth="106.5" text="District" />
                <TableColumn fx:id="phoneCol" prefWidth="141.5" text="Phone" />
                <TableColumn fx:id="propertyTypeCol" prefWidth="92.5" text="Property Type" />
                <TableColumn fx:id="locationCol" prefWidth="82.5" text="Location" />
                <TableColumn fx:id="radioButtonValueCol" prefWidth="86.0" text="Radio" />
            </columns>
        </TableView>
        <Button fx:id="updateListButton" layoutX="562.0" layoutY="542.0" mnemonicParsing="false" onAction="#SearchListButton" prefHeight="26.0" prefWidth="96.0" text="Update List" />
    </children>
</AnchorPane>
